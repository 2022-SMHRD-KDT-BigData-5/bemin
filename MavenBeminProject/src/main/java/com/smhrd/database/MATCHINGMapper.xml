<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.smhrd.domain.matchingDAO">
	
	<!-- 내역보기 -->
	<select id="selectMatching" resultType="com.smhrd.domain.MATCHING" parameterType="String">
		select * from MATCHING where USER_ID=#{USER_ID}
	</select>
	
	<!-- 페이징 -->
	<select id="selectAllMatching" resultType="com.smhrd.domain.MATCHING">

	</select>
	
	<!-- 레코드 갯수 -->
	<select id="MatchingCount" resultType="com.smhrd.domain.MATCHING">
		select count(user_id) count from (select ROWNUM mat_no from USER_ID=#{USER_ID})
	</select>
	
</mapper>
